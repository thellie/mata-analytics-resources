<properties>
	<body>
		<replacepostcontents/>
		<xpathpostcontents>.isi_artikel</xpathpostcontents>
	</body>
	<id>
		<xpathpostids>.title_news_detail &gt; h1:nth-child(1)</xpathpostids>
		<patternpostids/>
		<replacepostids/>
	</id>
	<source>
		<crawlname>Kompas News</crawlname>
	</source>
	<date>
		<xpathdates>div.editor_artikel_status_artikel:nth-child(1) &gt; div:nth-child(1) &gt; a:nth-child(2)</xpathdates>
		<dateformat>ddMMMyyyyy</dateformat>
		<replacedates>^(.*?),</replacedates>
		<attribute/>
	</date>
	<user>
		<xpathusers>div.editor_artikel_status_artikel:nth-child(1) &gt; div:nth-child(1) &gt; a:nth-child(1)</xpathusers>
		<replaceusers>kompas.com /|kompas.com/|:</replaceusers>
	</user>
	<header>
		<replaceheader/>
		<xpathheader>.title_news_detail &gt; h1:nth-child(1)</xpathheader>
	</header>
	<linkfollow>
		<patternlink>http://(.*?).kompas.com/read/.*</patternlink>
		<patternnextpage>http://search.kompas.com/.*</patternnextpage>
	</linkfollow>
	<outputdir>
		<outputdir>d:/Collections/BIN/kompasnews</outputdir>
	</outputdir>
	<manualcrawl>
		<booleanmanual>false</booleanmanual>
	</manualcrawl>
</properties>
