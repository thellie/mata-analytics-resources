<properties>
	<body>
		<xpathpostcontents>div[id~=post_message_.*]</xpathpostcontents>
		<replacepostcontents></replacepostcontents>
	</body>
	<id>
		<xpathpostids>#posts</xpathpostids>
		<patternpostids>&lt;table class="tborder" id="(.*)" cellpadding=(.*)&gt;</patternpostids>
		<replacepostids>&lt;table class="tborder" id="|" cellpadding=(.*)&gt;</replacepostids>
	</id>
	<source>
		<crawlname>VIVA Forum</crawlname>
	</source>
	<date>
		<xpathdates>.normal</xpathdates>
		<dateformat>ddMMMyyyyHH:mm</dateformat>
		<replacedates>,|WIB|WIT|WITA</replacedates>
		<attribute></attribute>
	</date>
	<user>
		<xpathusers>tr&gt;td&gt;div[id~=postmenu_.*]</xpathusers>
		<replaceusers></replaceusers>
	</user>
	<header>
		<xpathheader>td[id~=td_post_.*] &gt; div:nth-child(1) &gt; strong:nth-child(2)</xpathheader>
		<replaceheader></replaceheader>
	</header>
	<linkfollow>
		<patternlink></patternlink>
		<patternnextpage></patternnextpage>
	</linkfollow>
	<outputdir>
		<outputdir></outputdir>
	</outputdir>
	<manualcrawl>
		<booleanmanual>false</booleanmanual>
	</manualcrawl>
</properties>
