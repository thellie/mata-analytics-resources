	</user>
	<header>
		<replaceheader/>
		<xpathheader>.titles &gt; h1:nth-child(1)</xpathheader>
	</header>
	<linkfollow>
		<patternlink/>
		<patternnextpage/>
	</linkfollow>
	<outputdir>
		<outputdir>e:/Collections/BIN/okezone</outputdir>
	</outputdir>
