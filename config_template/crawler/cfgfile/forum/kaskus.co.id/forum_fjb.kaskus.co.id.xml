<properties>
	<id>
		<xpathpostids>div.listing-wrapper&gt;div[itemscope]</xpathpostids>
		<replacepostids>&lt;div class="row [a-z]+\-post" id="post|"&gt;</replacepostids>
		<patternpostids>&lt;div class="row [a-z]+\-post" id="post(.*?)"&gt;</patternpostids>
	</id>
	<body>
		<xpathpostcontents>div.entry-body&gt;div.entry</xpathpostcontents>
		<replacepostcontents/>
	</body>
	<source>
		<crawlname>Kaskus Forum</crawlname>
	</source>
	<date>
		<xpathdates>div.user-details&gt;time.entry-date</xpathdates>
		<dateformat>yyyy-MM-ddHH:mm:ss</dateformat>
		<replacedates>T</replacedates>
		<attribute>datetime</attribute>
	</date>
	<user>
		<xpathusers>div.user-details&gt;div.user-name&gt;a.nickname</xpathusers>
		<replaceusers/>
	</user>
	<header>
		<replaceheader/>
		<xpathheader>div.breadcrumbs&gt;div.current</xpathheader>
	</header>
</properties>
